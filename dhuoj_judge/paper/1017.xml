            <Solution xml:space="preserve" language="cpp" filename="test">#include&lt;iostream&gt;
using namespace std;
int main(){
	int n;
	while (cin&gt;&gt;n &amp;&amp; n){
		switch (n){
			case 3: 
			case 4: cout&lt;&lt;"One."&lt;&lt;endl; break;
			case 5: 
			case 6: cout&lt;&lt;"Two."&lt;&lt;endl; break;
			case 7: cout&lt;&lt;"Three."&lt;&lt;endl; break;
			default: cout&lt;&lt;"No."&lt;&lt;endl; break;
		}
	}
	return 0;
}</Solution>
