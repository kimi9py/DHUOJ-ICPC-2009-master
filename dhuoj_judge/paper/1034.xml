	while (cin &gt;&gt; n &amp;&amp; n &gt; 0){
		vector&lt;int&gt; a(n,0);
		for (int i = 0; i &lt; n; ++i)
			cin &gt;&gt; a[i];
		
		vector&lt;int&gt; result;
		for (int k = n; k &gt; 0; --k)
			result.insert (result.begin()+a[k-1], k);
		
		copy (result.begin(), result.end()-1, 
		ostream_iterator&lt;int&gt;(cout,","));
		cout &lt;&lt; result.back() &lt;&lt; endl;
	}
	return 0;
}
